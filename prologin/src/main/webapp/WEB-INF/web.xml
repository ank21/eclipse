<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>servsignup</servlet-name>
  	<display-name>servsignup</display-name>
  	<description></description>
  	<servlet-class>servsignup</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>servlogin</servlet-name>
  	<display-name>servlogin</display-name>
  	<description></description>
  	<servlet-class>servlogin</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>servforget</servlet-name>
  	<display-name>servforget</display-name>
  	<description></description>
  	<servlet-class>servforget</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>servsignup</servlet-name>
  	<url-pattern>/servsignup</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>servlogin</servlet-name>
  	<url-pattern>/servlogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>servforget</servlet-name>
  	<url-pattern>/servforget</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  <welcome-file>login.html</welcome-file>
  
  </welcome-file-list>
</web-app>
